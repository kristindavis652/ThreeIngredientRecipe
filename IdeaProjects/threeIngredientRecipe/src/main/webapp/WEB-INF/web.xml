<web-app>
  <display-name>CoderGirl 2019 Recipe Servlet</display-name>
  <servlet>
    <servlet-name>servlets.RecipeServlet</servlet-name>
    <servlet-class>servlets.RecipeServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>servlets.RecipeServlet</servlet-name>
    <url-pattern>/recipe</url-pattern>
  </servlet-mapping>


</web-app>
